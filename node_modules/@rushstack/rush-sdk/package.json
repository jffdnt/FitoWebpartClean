{
  "name": "@rushstack/rush-sdk",
  "version": "5.135.0-pr4927.1",
  "description": "An API for interacting with the Rush engine",
  "repository": {
    "type": "git",
    "url": "https://github.com/microsoft/rushstack.git",
    "directory": "apps/rush-sdk"
  },
  "homepage": "https://rushjs.io",
  "main": "lib-shim/index.js",
  "typings": "dist/rush-lib.d.ts",
  "exports": {
    ".": {
      "types": "./dist/rush-lib.d.ts",
      "default": "./lib-shim/index.js"
    },
    "./loader": {
      "types": "./dist/loader.d.ts",
      "default": "./lib-shim/loader.js"
    },
    "./lib/*": {
      "types": "./lib/*.d.ts",
      "default": "./lib/*.js"
    }
  },
  "typesVersions": {
    "*": {
      "loader": [
        "./dist/loader.d.ts"
      ]
    }
  },
  "license": "MIT",
  "dependencies": {
    "@types/node-fetch": "2.6.2",
    "tapable": "2.2.1",
    "@rushstack/node-core-library": "5.9.0",
    "@rushstack/lookup-by-path": "0.2.2",
    "@rushstack/terminal": "0.14.2"
  },
  "devDependencies": {
    "@types/semver": "7.5.0",
    "@types/webpack-env": "1.18.0",
    "webpack": "~5.82.1",
    "@microsoft/rush-lib": "5.135.0-pr4927.1",
    "@rushstack/heft": "0.67.2",
    "@rushstack/heft-webpack5-plugin": "0.10.12",
    "local-node-rig": "1.0.0",
    "@rushstack/stream-collator": "4.1.67",
    "@rushstack/ts-command-line": "4.22.8",
    "@rushstack/webpack-preserve-dynamic-require-plugin": "0.11.66"
  },
  "scripts": {
    "build": "heft build --clean",
    "_phase:build": "heft run --only build -- --clean",
    "_phase:test": "heft run --only test -- --clean"
  }
}